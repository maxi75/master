<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.layout.StackPane?>
<VBox xmlns:fx="http://javafx.com/fxml"
      fx:controller="de.hausknecht.master.frameworksanddrivers.ui.MenuBarController" fx:id="menuBar">

    <StackPane>
        <Button fx:id="openMenuButton" styleClass="mainButtons">
            <graphic>
                <ImageView fitWidth="20" fitHeight="20" preserveRatio="true">
                    <Image url="/packaging/open.png"/>
                </ImageView>
            </graphic>
        </Button>
    </StackPane>
</VBox>